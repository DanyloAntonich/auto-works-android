    <RelativeLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="-5dp"
        android:id="@+id/order_history_layout_summary"
        android:layout_below="@+id/history_layout_main">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Order:"
            android:id="@+id/textViewTotal"
            android:textColor="#000"
            android:textSize="14dp"
            android:layout_marginStart="27dp"
            android:layout_alignParentTop="true"
            android:layout_alignParentStart="true"
            android:textStyle="bold"
            android:layout_centerVertical="true" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Start"
            android:id="@+id/textView6"
            android:textColor="#000"
            android:textSize="14dp"
            android:layout_marginStart="10dp"
            android:layout_toRightOf="@+id/textViewTotal"
            android:layout_centerVertical="true" />

        <EditText
            android:layout_width="60dp"
            android:layout_height="30dp"
            android:inputType="number"
            android:ems="10"
            android:id="@+id/history_start"
            android:focusable="false"
            android:layout_toRightOf="@+id/textView6"
            android:layout_marginStart="10dp"
            android:background="@drawable/history_pink_dashed_edittext" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="End"
            android:id="@+id/textView7"
            android:textColor="#000"
            android:textSize="14dp"
            android:layout_marginStart="10dp"
            android:layout_toRightOf="@+id/history_start"
            android:layout_centerVertical="true" />

        <EditText
            android:layout_width="60dp"
            android:layout_height="30dp"
            android:inputType="number"
            android:ems="10"
            android:id="@+id/history_end"
            android:focusable="false"
            android:layout_toRightOf="@+id/textView7"
            android:layout_marginStart="10dp"
            android:background="@drawable/history_pink_dashed_edittext" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/history_clear"
            android:src="@drawable/history_clear"
            android:layout_toRightOf="@+id/history_end"
            android:layout_centerVertical="true"
            android:layout_marginStart="10dp" />

